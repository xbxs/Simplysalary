<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package ="entity">
	<class name="entity.Salary" table="SALARY">
		<id name="s_id" type="int" column="S_ID">
			<generator class="identity"></generator>
		</id>
		<property name="u_phone" type="java.lang.String"/>
		<property name="s_rtime" type="java.lang.String"/>
		<property name="s_term" type="java.lang.String"/>
		<property name="s_shift" type="java.lang.String"/>
		<property name="s_section" type="java.lang.String"/>
		<property name="s_wage" type="java.lang.String"/>	
	</class>
</hibernate-mapping>
